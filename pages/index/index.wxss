/* pages/index/index.wxss */

.loading-container{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.loading{
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -30rpx;
    margin-left: -30rpx;
}
.acknowledge{
    text-align: center;
    background-color: #ddd;
    font-size: 28rpx;
    line-height: 1.8;
}